<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hse.swb8.parkingSystem.pay.PayController">
    <children>
        <Label layoutX="14.0" layoutY="21.0" prefHeight="45.0" prefWidth="174.0" text="Bezahlen">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button fx:id="btnReadTicket" layoutX="375.0" layoutY="96.0" mnemonicParsing="false" onAction="#OnBtnReadTicketPressed" prefHeight="50.0" prefWidth="235.0" text="Auslesen" />
        <TextField fx:id="txtInfo" alignment="CENTER" editable="false" layoutX="120.0" layoutY="171.0" prefHeight="50.0" prefWidth="496.0" promptText="Informationen" />
        <TextField fx:id="txtTicketId" alignment="CENTER" layoutX="120.0" layoutY="96.0" prefHeight="50.0" prefWidth="235.0" promptText="Ticket Nummer" />
        <TextField fx:id="txtPrice" alignment="CENTER" editable="false" layoutX="120.0" layoutY="238.0" prefHeight="50.0" promptText="Preis" />
        <Button fx:id="btnPay" layoutX="381.0" layoutY="238.0" mnemonicParsing="false" onAction="#OnBtnPayPressed" prefHeight="50.0" prefWidth="235.0" text="Bezahlen" />
    </children>
</AnchorPane>
